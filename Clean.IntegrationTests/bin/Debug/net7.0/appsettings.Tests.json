{
  "Logging": {
    "LogLevel": {
      "Default": "Warning",
      "Umbraco.Cms.Tests": "Information"
    },
    "Console": {
      "DisableColors": true
    }
  },
  "Umbraco": {
    "CMS": {
      "Unattended": {
        "UpgradeUnattended": true,
        "InstallUnattended": true,
        "UnattendedUserName": "Test",
        "UnattendedUserEmail": "test@test.nl",
        "UnattendedUserPassword": "1234567890"
